<h2>Recherche d'auteurs</h2>
<form action="/authors" method="get">
    <label> Nom </label>
    <input type="text" name="name">
    <button type="submit">Rechercher</button>
</form>
<br>
<% if (locals.authors != null) { %> 
    <% authors.forEach(author => { %>
        <div><%= author.name %></div>
    <% }) %>
<% } %>

